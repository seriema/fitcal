{
  "name": "fitcal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon server.js",
    "test": "xo && mocha test/**/*.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.4.2",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "ical-generator": "^0.2.7",
    "moment-timezone": "^0.5.4",
    "mongoose": "^4.5.3",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-fitbit-oauth2": "^1.0.1",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "request": "^2.72.0",
    "shortid": "^2.2.6",
    "simple-oauth2": "^0.7.0"
  },
  "noAnalyze": true,
  "license": "Apache",
  "engines": {
    "node": "~6.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "xo": "^0.16.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "rules": {
      "no-warning-comments": "off",
      "xo/filename-case": "off",
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "rules": {
          "no-unused-expressions": "off"
        },
        "envs": [
          "mocha"
        ]
      }
    ]
  }
}
