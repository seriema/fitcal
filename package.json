{
  "name": "fitcal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon server.js",
    "test": "xo && mocha test/**/*.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "mongoose": "^4.11.0",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-fitbit-oauth2": "^1.0.2",
    "passport-local": "^1.0.0",
    "shortid": "^2.2.8"
  },
  "noAnalyze": true,
  "license": "Apache",
  "engines": {
    "node": "~6.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "mocha": "^3.4.0",
    "xo": "^0.18.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "rules": {
      "no-warning-comments": "off",
      "xo/filename-case": "off",
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "rules": {
          "no-unused-expressions": "off"
        },
        "envs": [
          "mocha"
        ]
      }
    ]
  }
}
